============================Gateway Conenctivity ======================

def Fuel_connectivity_check():
    logging.info("Verify fuel connectivity from jumphost via ping test")
    cmd = "ping -c 5 "+arg.FUEL_IP
    out = commands.getoutput(cmd)

    res = re.search("((\d){1,3})% packet loss",out)
    if int(res.group(1)) < 10:
        logging.info("connectivity verified between jmphost and " + arg.FUEL_IP)
        logging.info("Verify connectivity to fuel from jumphost -- PASSED")
    else:
        logging.info(arg.FUEL_IP + " fuel is not reachable from jmphost")
        raise Exception('fuel_connectivity_check -- FAILED')

def DCGW_connectivity_check():
====================================Fuel_connectivity_check() ===============
def Fuel_connectivity_check():
    logging.info("Verify fuel connectivity from jumphost via ping test")
    cmd = "ping -c 5 "+arg.FUEL_IP
    out = commands.getoutput(cmd)

    res = re.search("((\d){1,3})% packet loss",out)
    if int(res.group(1)) < 10:
        logging.info("connectivity verified between jmphost and " + arg.FUEL_IP)
        logging.info("Verify connectivity to fuel from jumphost -- PASSED")
    else:
        logging.info(arg.FUEL_IP + " fuel is not reachable from jmphost")
        raise Exception('fuel_connectivity_check -- FAILED')
