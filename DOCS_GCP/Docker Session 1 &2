============================re.findall&file.read===============================
def get_interface_list():
    os.system("sshpass -p 'ericsson' ssh admin@"+sys.argv[1]+" racadm hwinventory nic > /root/racadm.log")
    f = open('/root/racadm.log','r+')
    ret = f.read()
    niclist = re.findall('NIC.*\d-\d-\d',ret)
    print "List of Nics on server \n"+str(niclist)
    f.close()
    return niclist

=============================re.search&re.match,Group ===========================
def get_interface_pci_mac(interface):
    ret = os.system("sshpass -p 'ericsson' ssh admin@"+sys.argv[1]+" racadm hwinventory "+interface+" > /root/racadm.log")
    f2 = open('/root/racadm.log','r+')
    iface = f2.read()
    match = re.search('Permanent MAC Address:\s+([A-Fa-f0-9:]*)\n',iface)
    mac =  match.group(1)
    match2 = re.search('Bus Number:\s+(\d+)\n',iface)
    if len(match2.group(1))==1:
        bus_no =  '0'+match2.group(1)
    else:
        bus_no = hex(int(match2.group(1))).split('x')[-1]
    match3 = re.search('DeviceNumber:\s+(\d+)\n',iface)
    if len(match3.group(1))==1:
        dev_no =  '0'+match3.group(1)
    else:
        dev_no = match3.group(1)
    match4 =  re.search('Function Number:\s+(\d+)\n',iface)
    func_no = match4.group(1)
    pci_add = '0000:'+bus_no+':'+dev_no+'.'+func_no
    f2.close()
    return mac , pci_add

===============================Paramiko&module ===> Except Module ================================================

def getssh():
    try:
        client = paramiko.SSHClient()
        client.load_host_keys(os.path.expanduser('~/.ssh/known_hosts'))
        client.set_missing_host_key_policy(paramiko.AutoAddPolicy())
        client.connect(sys.argv[1], port=22, username='admin', password='ericsson')
    except:
        pass
    finally:
        client.close()
